{
  "expo": {
    "name": "Primotex Técnico",
    "slug": "primotex-tecnico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E8B57"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.primotex.tecnico",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Este app precisa acessar a câmera para capturar fotos das ordens de serviço.",
        "NSLocationWhenInUseUsageDescription": "Este app precisa acessar a localização para registrar o local das ordens de serviço.",
        "NSMicrophoneUsageDescription": "Este app pode precisar acessar o microfone para gravação de áudio.",
        "NSPhotoLibraryUsageDescription": "Este app precisa acessar a galeria para selecionar fotos.",
        "NSFaceIDUsageDescription": "Use Face ID para fazer login rapidamente no app.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2E8B57"
      },
      "package": "com.primotex.tecnico",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY_HERE"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-splash-screen",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2E8B57",
          "sounds": [
            "./assets/notification.wav"
          ]
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Use Face ID para fazer login rapidamente."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir acesso à câmera para capturar fotos das OS."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir acesso à localização para registrar local das OS."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir acesso à galeria para selecionar fotos.",
          "savePhotosPermission": "Permitir salvar fotos capturadas na galeria."
        }
      ],
      "expo-sqlite",
      "expo-image-picker",
      "expo-document-picker",
      "expo-file-system"
    ],
    "scheme": "primotex-tecnico",
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "owner": "primotex-company",
    "privacy": "unlisted",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/your-project-id-here"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}